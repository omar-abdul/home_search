    "npm": "^8.19.2",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.7.3",
    "run": "^1.4.0",
    "start-dev": "^1.0.0",
    "uuid": "^8.3.2",
