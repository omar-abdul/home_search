{
  "name": "guriraadi",
  "version": "1.0.0",
  "description": "Search for houses in somalia",
  "main": "app.js",
  "scripts": {
    "test": "mocha --require ts-node/register -r tsconfig-paths/register  src/tests/**/*.spec.ts",
    "build": "tsc --watch",
    "serve": "nodemon dist/app.js",
    "start": "node dist/app.js",
    "start-dev": "nodemon --legacy-watch -r tsconfig-paths/register app.ts",
    "coverage": "nyc npm run test",
    "knex:up": "knex --knexfile src/data-access/config/knex.ts  migrate:latest ",
    "knex:down": "knex --knexfile src/data-access/config/knex.ts  migrate:down"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ]
  },
  "author": "Omz",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "argon2": "^0.30.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "file-type": "^18.3.0",
    "firebase-admin": "^11.7.0",
    "joi": "^17.9.2",
    "knex": "^2.4.2",
    "knex-postgis": "^0.14.3",
    "libphonenumber-js": "^1.10.28",
    "multer": "1.4.5-lts.1",
    "npm": "^8.19.4",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.10.0",
    "read-chunk": "^4.0.3",
    "uuid": "^8.3.2",
    "validatorjs": "^3.22.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.3",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/sinon": "^10.0.14",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^7.6.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "sinon": "^14.0.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
